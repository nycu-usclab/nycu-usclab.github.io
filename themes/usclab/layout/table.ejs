<main class="table">
  <div class="container">
    <h1><%- page.title %></h1>
    <table id="datatable" class="display"></table>
    <script>
      Papa.parse("index.csv", {
        download: true,
        complete: (result) => {
          const data = result.data.filter(i => i.join() != "")
          const title = data.shift().map((x) => ({ title: x }));
          
          $.fn.dataTable.ext.errMode = 'none';
          $("#datatable").DataTable({
            data: data,
            columns: title,
          });
        },
      });
    </script>
  </div>
</main>
