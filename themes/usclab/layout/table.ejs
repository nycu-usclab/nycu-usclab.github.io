<div class="container">
  <h1><%- page.title %></h1>
  <table class="display"></table>
  <script>
    Papa.parse("<%- url_for(page.csv) %>", {
      download: true,
      complete: (result) => {
        const data = result.data;
        const title = data.shift().map((x) => ({ title: x }));
        $("table").DataTable({
          data: data,
          columns: title,
        });
      },
    });
  </script>
</div>
