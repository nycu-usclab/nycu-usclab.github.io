<main class="gallary">
  <div class="container">
    <h1><%- page.title %></h1>
    <div class="row">
      <% page.images.forEach(p => { %>
      <div class="col-lg-3 col-md-4 col-sm-6">
        <a href="<%- p.image %>" target="_blank">
          <div
            class="pic"
            style="background-image: url('<%- p.image %>')"
          ></div>
        </a>
        <p class="text-center mt-2"><%- p.name %></p>
        <p></p>
      </div>

      <% }) %>
    </div>
  </div>
</main>
